{
  "name": "zjson",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "node build/pre-build.js && ng serve --proxy-config proxy.conf.json -o",
    "build": "node build/pre-build.js && ng build --prod",
    "build:electron": "node build/pre-build.js electron && ng build --prod",
    "update": "npm run build && node build/update-zjson.js",
    "electron:dist": "electron dist",
    "electron": "npm run build:electron && node build/electron.conf.js && electron dist",
    "electron:build": "npm run build:electron && node build/electron.conf.js package",
    "electron:package:win": "electron-packager dist zjson --platform=win32 --out=build/package --icon=dist/electron/zjson.ico --overwrite",
    "electron:package:mac": "electron-packager dist zjson --platform=darwin --out=build/package --icon=dist/electron/zjson.ico --overwrite",
    "electron:package:macm1": "electron-packager dist zjson --platform=mas --out=build/package --icon=dist/electron/zjson.ico --overwrite",
    "package:win": "npm run electron:build && npm run electron:package:win && node build/pre-build.js",
    "package:mac": "npm run electron:build && npm run electron:package:mac && node build/pre-build.js && npm run make:dmg",
    "package:macm1": "npm run electron:build && npm run electron:package:macm1 && node build/pre-build.js && npm run make:dmg",
    "make:dmg": "electron-installer-dmg build/package/zjson-darwin-x64 zjson-darwin-x64",
    "make:dmgm1": "electron-installer-dmg build/package/zjson-mas-x64 zjson-mas-x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "funclib": "^3.5.8",
    "jquery": "^3.3.1",
    "monaco-editor": "^0.14.3",
    "ngx-bootstrap": "^3.0.1",
    "request": "^2.88.0",
    "rxjs": "6.0.0",
    "rxjs-compat": "6.1.0",
    "unzip": "^0.1.11",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "electron": "^2.0.18",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^12.1.0",
    "electron-wix-msi": "^3.2.0",
    "glob": "^7.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "progress": "^2.0.0",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "value-looker": "^2.1.2"
  }
}
